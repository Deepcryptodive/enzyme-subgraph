  "devdoc": {
    "author": "Melonport AG <team@melonport.com>",
    "methods": {
      "constructor": {
        "details": "Define and register a quote asset against which all prices are measured/based against"
      },
      "existsPriceOnAssetPair(address,address)": {
        "details": "Prices are only upated against QUOTE_ASSET",
        "params": {
          "buyAsset": "Asset for which check to be done if data exists",
          "sellAsset": "Asset for which check to be done if data exists"
        }
      },
      "getInvertedPriceInfo(address)": {
        "details": "Asset has been initialised and its price is non-zero",
        "params": {
          "_asset": "Asset for which inverted price should be return"
        },
        "returns": {
          "assetDecimals": "Decimal places for this asset }",
          "invertedPrice": "Price based (instead of quoted) against QUOTE_ASSET"
        }
      },
      "getKyberMaskAsset(address)": {
        "details": "Get Kyber representation of ETH if necessary"
      },
      "getOrderPriceInfo(address,uint256,uint256)": {
        "params": {
          "buyQuantity": "Quantity in base units being bought of buyAsset",
          "sellAsset": "Address of the asset to be sold",
          "sellQuantity": "Quantity in base units being sold of sellAsset"
        },
        "returns": {
          "orderPrice": "Price as determined by an order"
        }
      },
      "getPrice(address)": {
        "details": "Asset has been registered",
        "params": {
          "_asset": "Asset for which price should be returned"
        },
        "returns": {
          "price": "Price formatting: mul(exchangePrice, 10 ** decimal), to avoid floating numbers",
          "timestamp": "When the asset's price was updated }"
        }
      },
      "getReferencePriceInfo(address,address)": {
        "params": {
          "_baseAsset": "Address of base asset",
          "_quoteAsset": "Address of quote asset"
        },
        "returns": {
          "decimals": "Decimal places for quoteAsset }",
          "referencePrice": "Quantity of quoteAsset per whole baseAsset"
        }
      },
      "update()": {
        "details": "Stores zero as a convention for invalid price"
      }
    },
    "title": "Price Feed Template"
  },
  "userdoc": {
    "methods": {
      "convertQuantity(uint256,address,address)": {
        "notice": "Get quantity of toAsset equal in value to given quantity of fromAsset"
      },
      "existsPriceOnAssetPair(address,address)": {
        "notice": "Checks whether data exists for a given asset pair"
      },
      "getInvertedPriceInfo(address)": {
        "notice": "Gets inverted price of an asset"
      },
      "getKyberPrice(address,address)": {
        "notice": "Returns validity and price from Kyber"
      },
      "getOrderPriceInfo(address,uint256,uint256)": {
        "notice": "Gets price of Order"
      },
      "getPrice(address)": {
        "notice": "Gets price of an asset multiplied by ten to the power of assetDecimals"
      },
      "setMaxSpread(uint256)": {
        "notice": "_maxSpread becomes a percentage when divided by 10^18(e.g. 10^17 becomes 10%)"
      }
    },
    "notice": "Routes external data to smart contractsWhere external data includes sharePrice of Melon fundsPriceFeed operator could be staked and sharePrice input validated on chain"
  }
