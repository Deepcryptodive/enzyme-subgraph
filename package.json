    "@melonproject/protocol-v.1.0.1": "npm:@melonproject/protocol@1.0.1",
    "commander": "^4.0.0",
    "glob": "^7.1.5",
    "inquirer": "^7.0.0",
    "mustache": "^3.1.0"
  },
